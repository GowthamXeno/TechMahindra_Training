<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>CookiesSample</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  
    <servlet>
  <servlet-name>CreateCook</servlet-name>
  <servlet-class>Cookies.CreateCookieServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>CreateCook</servlet-name>
  <url-pattern>/CreateCookieServlet</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>GetCook</servlet-name>
  <servlet-class>Cookies.GetCookieServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>GetCook</servlet-name>
  <url-pattern>/GetCookieServlet</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>DeleteCook</servlet-name>
  <servlet-class>Cookies.DeleteCookieServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>DeleteCook</servlet-name>
  <url-pattern>/DeleteCookieServlet</url-pattern>
  </servlet-mapping>
</web-app>